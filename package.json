  "bugs": {
    "url": "https://github.com/fangmd/leetcode/issues"
  },
  "homepage": "https://github.com/fangmd/leetcode#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
